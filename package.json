{
  "name": "inst-visualiser-uwb",
  "version": "3.0.0",
  "description": "Ultra-fast UWB (Ultra-Wideband) position visualiser with real-time physics simulation and MQTT device control",
  "main": "index.html",
  "scripts": {
    "start": "python -m http.server 8080",
    "serve": "python3 -m http.server 8080",
    "dev": "npx live-server --port=8080 --open=/",
    "test": "echo \"Open index.html in browser or use npm run serve\"",
    "validate": "html5validator index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DynamicDevices/inst-visualiser.git"
  },
  "keywords": [
    "uwb",
    "ultra-wideband",
    "positioning",
    "visualization",
    "mqtt",
    "real-time",
    "physics",
    "simulation",
    "mass-spring",
    "iot",
    "embedded",
    "wireless",
    "tracking",
    "location",
    "dynamic-devices"
  ],
  "author": {
    "name": "Dynamic Devices Ltd",
    "email": "info@dynamicdevices.co.uk",
    "url": "https://www.dynamicdevices.co.uk"
  },
  "license": "GPL-3.0",
  "licenses": [
    {
      "type": "GPL-3.0",
      "url": "https://www.gnu.org/licenses/gpl-3.0.html"
    }
  ],
  "bugs": {
    "url": "https://github.com/DynamicDevices/inst-visualiser/issues"
  },
  "homepage": "https://dynamicdevices.github.io/inst-visualiser/",
  "engines": {
    "node": ">=12.0.0"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "not dead"
  ],
  "devDependencies": {
    "live-server": "^1.2.2",
    "html5validator": "^0.4.2"
  },
  "files": [
    "index.html",
    "README.md",
    "LICENSE",
    "resources/"
  ],
  "config": {
    "default_mqtt_host": "mqtt.dynamicdevices.co.uk",
    "default_mqtt_port": 8083,
    "default_topic": "uwb/positions"
  },
  "funding": {
    "type": "individual",
    "url": "https://www.dynamicdevices.co.uk"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  }
}